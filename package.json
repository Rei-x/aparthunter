    "@japa/assert": "^3.0.0",
    "@japa/browser-client": "^2.0.3",
    "@japa/expect": "^3.0.2",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@japa/snapshot": "^2.0.6",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@swc/core": "^1.7.22",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.5.1",
    "autoprefixer": "^10.4.20",
