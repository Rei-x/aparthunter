{
  "name": "apart-hunter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:next": "SKIP_ENV_VALIDATION=true next build",
    "build:esbuild": "tsx esbuild.config.ts",
    "build": "pnpm run build:next && pnpm run build:esbuild",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "setup": "tsx ./src/setup.ts",
    "test": "vitest",
    "generate": "pnpm run codegen && pnpm prisma generate",
    "db:studio": "prisma studio",
    "dev": "pnpm run setup && tsx watch -r dotenv/config src/server.ts  --project tsconfig.server.json ",
    "postinstall": "if [ -z $CI ]; then prisma generate; fi",
    "lint": "next lint",
    "docker:build": "./docker/build.sh",
    "docker:push": "./docker/push.sh",
    "start": "pnpm db:migrate && NODE_ENV=production pm2-runtime ./dist/server.mjs",
    "codegen": "graphql-codegen --config codegen.ts"
  },
