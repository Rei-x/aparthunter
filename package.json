    "lodash.isequal": "^4.5.0",
    "lucide-svelte": "^0.447.0",
    "luxon": "^3.5.0",
    "openapi-types": "^12.1.3",
    "pg": "^8.13.0",
    "pino-pretty": "^11.2.2",
    "pm2": "^5.4.2",
    "postcss": "^8.4.47",
    "reflect-metadata": "^0.2.2",
    "svelte": "^4.2.19",
    "tailwindcss": "^3.4.13",
    "zod": "^3.23.8"
